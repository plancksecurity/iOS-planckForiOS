import Foundation

struct Account {
    var email: String
    var imapServerName: String
    var smtpServerName: String
    var password: String
    var imapPort: Int
    var smtpPort: Int

    /**
     This unfortunately must correspond to the localized name on screen.
     */
    var imapTransportSecurityString: String

    /**
     This unfortunately must correspond to the localized name on screen.
     */
    var smtpTransportSecurityString: String
}

struct UITestData {
    static let workingAccount = Account.init(
        email: "email",
        imapServerName: "server", smtpServerName: "server",
        password: "password", imapPort: 993, smtpPort: 587,
        imapTransportSecurityString: "TLS", smtpTransportSecurityString: "StartTLS")

    static let workingYahooAccount = Account.init(
        email: "email",
        imapServerName: "server", smtpServerName: "server",
        password: "password", imapPort: 993, smtpPort: 587,
        imapTransportSecurityString: "TLS", smtpTransportSecurityString: "TLS")
}
